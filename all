#coding=utf-8
import time,os
os.system('clear')

#####logo#####
logo = '''
\033[1;92m                        

              $$$$$$\  $$\ $$\ 
             $$  __$$\ $$ |$$ |
             $$ /  $$ |$$ |$$ |
             $D$E$A$D |M$A|N$ |
             $$  __$$ |$$ |$$ |
             $$ |  $$ |$$ |$$ |
